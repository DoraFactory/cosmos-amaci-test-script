{
	"name": "cosmos-maci-set-reward-script",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"restart": "rm -rf progress.json && ts-node --files src/index.ts",
		"start": "ts-node --files src/index.ts",
		"test": "ts-node --files src/test.ts",
		"distribute": "ts-node --files src/distributeToken.ts",
		"deactive": "ts-node --files src/run-deactive-test.ts",
		"no-deactive": "ts-node --files src/run-non-deactive-test.ts",
		"test:deactive": "DEACTIVATE=true ts-node --files src/run-test.ts",
		"test:no-deactive": "DEACTIVATE=false ts-node --files src/run-test.ts",
		"log:deactive": "cross-env LOG_FILE=deactive.log npm run deactive",
		"log:no-deactive": "cross-env LOG_FILE=no-deactive.log npm run no-deactive",
		"log:all": "npm-run-all --sequential log:deactive log:no-deactive"
	},
	"dependencies": {
		"@cosmjs/amino": "^0.32.1",
		"@cosmjs/cosmwasm-stargate": "^0.32.1",
		"@cosmjs/crypto": "^0.32.1",
		"@cosmjs/launchpad": "^0.27.1",
		"@cosmjs/proto-signing": "^0.32.1",
		"@cosmjs/stargate": "^0.32.1",
		"@injectivelabs/sdk-ts": "^1.14.33",
		"bech32": "^1.1.4",
		"blake-hash": "^2.0.0",
		"chalk": "^5.0.1",
		"circom": "https://github.com/weijiekoh/circomlib.git#24ed08eee0bb613b8c0135d66c1013bd9f78d50a",
		"cosmjs-types": "^0.9.0",
		"csv-parse": "^5.5.6",
		"dotenv": "^16.3.1",
		"ffjavascript": "^0.2.60",
		"pg": "^8.11.5",
		"ts-node": "^10.9.1",
		"tsup": "^6.7.0",
		"tsx": "^3.12.6",
		"typescript": "5.1.6",
		"yargs": "^17.7.1",
		"zod": "^3.21.4",
		"zod-validation-error": "^1.3.0"
	},
	"devDependencies": {
		"@types/pg": "^8.11.6",
		"@types/progress": "^2.0.7",
		"@types/readline-sync": "^1.4.8",
		"@types/tar": "^6.1.10",
		"@types/yargs": "^17.0.24",
		"snarkjs": "^0.7.0"
	},
	"include": [
		"src/**/*",
		"src/@types/index.d.ts"
	]
}
